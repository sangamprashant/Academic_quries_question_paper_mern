    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "firebase": "^10.7.1",
    "mongoose": "^8.0.3",
    "next": "14.0.4",
    "nodemailer": "^6.9.8",
    "react": "^18",
    "react-bootstrap": "^2.9.2",
    "react-dom": "^18",
    "react-toastify": "^9.1.3",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "electron": "^30.0.3",
